<UserControl x:Class="FN.Framtal.UserControls.NumericTextBoxWithTextBoxWhiteText"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FN.Framtal.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="25" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1.5*" />
            <ColumnDefinition Width="1.5*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <local:WhiteContentWithDescription x:Name="WhiteTextBlock" Grid.ColumnSpan="2"  Margin="0,0,5,0"/>
        <TextBox Grid.Column="1"  Margin="1,1,5,1"></TextBox>
        <local:NumericTextBox Background="LightGray" Grid.Column="2" />
        

    </Grid>
</UserControl>
